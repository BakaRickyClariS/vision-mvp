datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}


model VisionResult {
  id         Int      @id @default(autoincrement())
  type       String
  imageUrl   String
  resultJson Json
  createdAt  DateTime @default(now())
}
